<!DOCTYPE html>
<html>
  <head>
    <title>TomoYoga</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.9.0/css/all.css">
  </head>

  <body>
    <%= render 'layouts/header' %>
    <table class="table">
      <thead>
        <tr class="table-1">
          <th><img class="img-1" src="/assets/yoga/meditate-1851165_640.jpg" width="450"></th>
          <th><img class="img-2" src="/assets/yoga/people-2573216_640.jpg" width="450"></th>
          <th><img class="img-3" src="/assets/yoga/rock-731140_640.jpg" width="450"></th>
          <th><img class="img-4" src="/assets/yoga/yoga-2662234_640.jpg" width="450"></th>
        </tr>
      </thead>
    </table>

    <div class = 'container fluid' >
   
      <div class="row">
        <%= yield %>
      </div>
    </div><br><br><br>
    <%= render 'layouts/footer' %>
  </body>
</html>
